
config quality 'signal'
	option excellent_min_snr '25'
	option excellent_color 'green'
	option good_min_snr '20'
	option good_color 'blue'
	option average_min_snr '3'
	option average_color 'yellow'
	option bad_min_snr '0'
	option bad_color 'red'
	option bad_blink '1'

config led_control 'station'
	option enable_power '1'
	option enable_5g '1'
	option enable_mobile_signal '1'
	option enable_wifi '1'
	option enable_internet '1'
	option enable_phone '1'

